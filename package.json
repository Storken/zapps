{
  "name": "zapps",
  "version": "1.0.0",
  "description": "Zapps -- Zero-Dependency Applications",
  "main": "index.js",
  "scripts": {
    "serve": "npx @11ty/eleventy --serve",
    "build": "npx @11ty/eleventy",
    "test": ".site/_tools/hyperlink public/ --check-anchors --sources=./src",
    "fetch-hyperlink": "mkdir -p .site/_tools && run-script-os",
    "fetch-hyperlink:darwin": "wget -q https://github.com/untitaker/hyperlink/releases/download/0.1.15/hyperlink-mac-x86_64 -O .site/_tools/hyperlink && chmod +x .site/_tools/hyperlink",
    "fetch-hyperlink:linux": "wget -q https://github.com/untitaker/hyperlink/releases/download/0.1.15/hyperlink-linux-x86_64 -O .site/_tools/hyperlink && chmod +x .site/_tools/hyperlink"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/warpsys/zapps.git"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0 OR MIT",
  "bugs": {
    "url": "https://github.com/warpsys/zapps/issues"
  },
  "homepage": "https://github.com/warpsys/zapps#readme",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "run-script-os": "^1.1.6"
  }
}
